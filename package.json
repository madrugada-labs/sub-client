{
  "name": "sub-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "precommit": "lint-staged",
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "prettify": "prettier --write './**/*.{js,jsx,ts,tsx,scss,md,json}' --config ./.prettierrc",
    "lint": "next lint -- --fix",
    "prettier": "./node_modules/.bin/prettier ./ --write",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@headlessui/react": "^1.6.6",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.4",
    "lodash": "^4.17.21",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-cool-img": "^1.2.12",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.34.0",
    "react-textarea-autosize": "^8.3.4",
    "react-toastify": "^9.0.7",
    "sass": "^1.54.0",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/fragment-matcher": "^3.3.0",
    "@graphql-codegen/introspection": "^2.2.0",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-codegen/typescript-react-apollo": "^3.3.2",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql-codegen-apollo-next-ssr": "^1.7.4",
    "husky": "^8.0.1",
    "import-sort-style-react": "^5.1.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "tailwindcss": "^3.1.7",
    "type-fest": "^2.18.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js}": [
      "prettier --write"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "import-sort-style-react"
    }
  },
  "engines": {
    "node": ">=14"
  }
}
